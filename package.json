{
  "name": "mongoose-find-by-reference",
  "author": "CheezOne",
  "homepage": "https://github.com/cheezone/mongoose-find-by-reference",
  "version": "1.0.6",
  "description": "Make Mongoose support finding in reference fields.",
  "main": "dist/main.js",
  "module": "dist/main.mjs",
  "types": "dist/main.d.ts",
  "scripts": {
    "start": "npx ts-node src/main.ts",
    "build": "tsup",
    "release": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheezone/mongoose-find-by-reference.git"
  },
  "bugs": {
    "url": "https://github.com/cheezone/mongoose-find-by-reference/issues"
  },
  "keywords": [
    "mongoose",
    "reference",
    "ref",
    "objectid",
    "mongodb"
  ],
  "license": "Apache 2.0",
  "peerDependencies": {
    "mongoose": ">=6.2.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.77",
    "eslint": "^8.47.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "<5.2"
  },
  "dependencies": {
    "mocha": "^10.2.0"
  },
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "clean": true,
    "dts": true
  }
}
